<script type="text/javascript">

function endsWith(s,sub) {
	try{
	var numchars = s.length-sub.length
	if ((s.trim() != "") && (sub.trim() != "") && (s.substring(numchars,s.length) == sub)) return true;
	else return false;
		}
		catch (e) {
		alert(s+' - '+sub);
		}
}

function isEmail(s) {
	var pat = /[^A-Za-z0-9.@\-_]+/;
	if (!(pat.test(s)) && (s.split('@').length == 2) && (s.indexOf('@') > 0) && (s.charAt(s.indexOf('@')-1) != '.') && (s.charAt(s.indexOf('@')+1) != '.') && (endsWith(s,'.com') || endsWith(s,'.net')
	|| endsWith(s,'.org') || endsWith(s,'.edu') || endsWith(s,'.gov') || endsWith(s,'.mil') || endsWith(s,'.us'))) return true;
    else return false;
}

</script>
      

