<script src="/responsive2017/js/responsiveslides.min.js"></script>

<script type="text/javascript">
$(document).ready(function() {

/***************
* SLIDES       *
****************/
$("#ihs-responsive-slider").responsiveSlides({
  auto: true,             // Boolean: Animate automatically, true or false
  speed: 1500,            // Integer: Speed of the transition, in milliseconds
  //timeout: 4000,          // Integer: Time between slide transitions, in milliseconds
  timeout: 6000,          // Integer: Time between slide transitions, in milliseconds
  pager: false,           // Boolean: Show pager, true or false
  nav: true,             // Boolean: Show navigation, true or false
  random: false,          // Boolean: Randomize the order of the slides, true or false
  pause: false,           // Boolean: Pause on hover, true or false
  pauseControls: true,    // Boolean: Pause when hovering controls, true or false
  prevText: "Previous",   // String: Text for the "previous" button
  nextText: "Next",       // String: Text for the "next" button
  maxwidth: "",           // Integer: Max-width of the slideshow, in pixels
  navContainer: "",       // Selector: Where controls should be appended to, default is after the 'ul'
  manualControls: "",     // Selector: Declare custom pager navigation
  namespace: "callbacks",   // String: Change the default namespace used
  before: function(){},   // Function: Before callback
  after: function(){}     // Function: After callback
});

/************************
* header drop-down menu *
*************************/
	$('.topNav-dd').hide(false,true);
    var $mainNav = $('#CORE_topNav');
    navWidth = $mainNav.width();
    $mainNav.children('.topNav-item, .topNav-item-chosen').bind('mouseenter focusin', function (event){
    	var $this = $(this);
    	$dd = $this.find('.topNav-dd');
		$this.find('a').removeClass('toplink');
    	var leftPos = $this.find('.topNav-tab').position().left;
    	var ddWidth = $dd.width();
    	leftMax = navWidth - ddWidth;
    	$dd.css('left', Math.min(leftPos, leftMax)).show();
    	$(this).addClass('topNav-item-active');
	$mainNav.children('.topNav-item-active').bind('mouseleave', function (event){
		$dd.hide();
		$(this).removeClass('topNav-item-active');
					$this.find('a').addClass('toplink');
	});
	// focusout for keyboard navigation
	$('.topNav-dd').find('div:last-child ul li:last-child a').focusout(function(){
		$dd.hide();
		$('.topNav-item-active').removeClass('topNav-item-active');
	});
	});


jQuery('ul.nav li.dropdown').hover(function() {
  jQuery(this).find('.dropdown-menu').stop(true, true).delay(250).show(0);
}, function() {
  jQuery(this).find('.dropdown-menu').stop(true, true).delay(0).hide(0);
});

/**********************
* header A to Z index *
***********************/
	$('.CORE_aToZclick').show();
	$('.CORE_aToZ_open').hide();
	$('.CORE_aToZclick').click(function(){
		$('.CORE_aToZ_open').toggle();
		$('.CORE_aToZclickActive').not(this).toggleClass('CORE_aToZclickActive').toggleClass('CORE_aToZclick');
		$(this).toggleClass('CORE_aToZclickActive').toggleClass('CORE_aToZclick');
	});
	$('.aToZ_close').click(function(){
		$('.CORE_aToZ_open').hide();
		$('.CORE_aToZclickActive').toggleClass('CORE_aToZclick').toggleClass('CORE_aToZclickActive');
	});

/************
* Login box *
*************/

	$('.CORE_loginOpen').hide();
	$('.loginOff').click(function(){
		$('.CORE_loginOpen').fadeToggle(100);
		$('.loginOn').not(this).toggleClass('loginOn').toggleClass('loginOff');
		$(this).toggleClass('loginOn').toggleClass('loginOff');
	});

	$('.loginActive').click(function(){
		$('.CORE_loginOpen').fadeToggle(100);
		$('.logoffOn').not(this).toggleClass('logoffOn').toggleClass('loginActive');
		$(this).toggleClass('logoffOn').toggleClass('loginActive');
	});

	$('#closebutton').click(function(){
		$('#loggedin').addClass('loginActive');
		$('#loggedin').removeClass('logoffOn');
		$('.CORE_loginOpen').hide();
	});

	$('#cancelbutton').click(function(){
		$('#txt').removeClass('loginOn');
		$('#txt').addClass('loginOff');
	$('.CORE_loginOpen').hide();

	});

/***********
* fancybox *
***********/
// Image galleries
    //FANCYBOX ADDED 080917 JDR
    $(".fancybox-thumb,.lightBox").each( function(){
        $(this).attr('data-caption',$(this).attr('title'));
        $(this).attr('data-fancybox',$(this).attr('rel'));
     });
	$(".fancybox-thumb,.lightBox").fancybox();


/***************
* Hidden lists *
****************/
	$('.CORE_linkShow').hide();
	$('.CORE_linkCollapsed').click(function(){
		$(this).next('.CORE_linkShow').slideToggle(200);
	});
/*********
* Tables *
**********/
// table vertical zebra striping
	$('.CORE_vertTableStripes tr td:nth-child(even)').addClass('altStripes');


/****************
* Scroll to TOP *
****************/
	$(window).scroll(function(){
		if ($(this).scrollTop() > 100) {
			$('.scrollToTop').fadeIn();
		} else {
			$('.scrollToTop').fadeOut();
		}
	});
	$('.scrollToTop').click(function(){
		$('html, body').animate({scrollTop : 0},800);
		return false;
	});

/********************
* TOGGLE INDICATOR *
********************/
/* ADDED BY D.BLACKBIRD 5-22-2017 */
	$('.collapse').on('shown.bs.collapse', function(){
	$(this).parent().find(".fa-plus").removeClass("fa-plus").addClass("fa-minus");
	}).on('hidden.bs.collapse', function(){
	$(this).parent().find(".fa-minus").removeClass("fa-minus").addClass("fa-plus");
	});

/********************
* OPEN/CLOSE ALL    *
********************/

/* ADDED BY D.BLACKBIRD 5-26-2017 */
    $(".btn-open").click(function(){
        $('.collapse').collapse('show');
		$('.collapsed').collapse('show');
		$('.collapse').attr("aria-hidden", "false");
		$('.collapsed').attr("aria-hidden", "false");
		$('.collapsed').switchClass( "collapsed", "collapse");
		$('.usa-accordion-content').attr("aria-hidden", "false");
		$('.usa-accordion-button').attr("aria-expanded", "true");
    });
    $(".btn-close").click(function(){
        $('.collapse.in').collapse('hide');
		$('.collapse').attr("aria-hidden", "true");
		$('.collapsed').attr("aria-hidden", "true");
		$('.usa-accordion-content').attr("aria-hidden", "true");
		$('.usa-accordion-button').attr("aria-expanded", "false");
    });

/***************
* NEW BADGE    *
****************/
	$(".DateDiv").each(function(index) {
		var sRange = $(this).attr("data-daterange");
		var arrTemp = sRange.split(" to ");
		var dtFrom = new Date(arrTemp[0]);
		var dtTo = new Date(arrTemp[1]);
		var dtNow = new Date();
		if (dtNow >= dtFrom && dtNow <= dtTo){
			$(this).find('.new-label').fadeIn('slow');
		}
	});

/******************************
* TABBABLE BOOTSTRAP NAVBAR   *
*******************************/
	$("#menu1,#menu2,#menu3,#menu4,#menu5,#menu6,#menu7").on("focus",function() {
		$("#menu1,#menu2,#menu3,#menu4,#menu5,#menu6,#menu7").not(this).removeClass("active");
		$(this).addClass("active");
		$(this).next(".dropdown-menu").collapse('show');
		$(".dropdown-menu").not(this).collapse('hide');
	});

/******************************
* ADD TO CALENDAR   *
*******************************/
	(function () {
	if (window.addtocalendar)if(typeof window.addtocalendar.start == "function")return;
	if (window.ifaddtocalendar == undefined) { window.ifaddtocalendar = 1;
	var d = document, s = d.createElement('script'), g = 'getElementsByTagName';
	s.type = 'text/javascript';s.charset = 'UTF-8';s.async = true;
	s.src = ('https:' == window.location.protocol ? 'https' : 'http')+'://ihs.gov/responsive2017/js/atc.min.js';
	var h = d[g]('body')[0];h.appendChild(s); }
	})();

	// bind a click event to the 'skip' link
	$("#skipNav").click(function(event){

	// strip the leading hash and declare
	// the content we're skipping to
	var skipTo="#"+this.href.split('#')[1];

	// Setting 'tabindex' to -1 takes an element out of normal
	// tab flow but allows it to be focused via javascript
	$(skipTo).attr('tabindex', -1).on('blur focusout', function () {

		// when focus leaves this element,
		// remove the tabindex attribute
		$(this).removeAttr('tabindex');

	}).focus(); // focus on the content container
	});

	// add title to add this + symbol
	$('.addthis_button_compact').attr('title', 'More Sharing Services');

});
</script>




