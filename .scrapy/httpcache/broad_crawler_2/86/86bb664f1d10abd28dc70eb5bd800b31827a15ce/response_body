<script>





  //This does the leftnav highlighting on three items that are on a sub sub level, these are coded this way because I orginally didn't plan to have the leftnav highlight two levels down
  if (window.location.href.indexOf("prepare-to-apply-and-register") > -1) {

    $(".pta").parent().toggleClass('hide-item');
    $(".submenu").toggleClass("selected-submenu");
    $(".pta").toggleClass("selected-item");
  }


  if (window.location.href.indexOf("foreign/") > -1) {
          $(".hide-fg").toggleClass("hide-fg");
          $(".fg").addClass("leftnav-active");
          $(".leftmenu-item-nosub, .gs, .hoa, .arar, .papp, .res, .resa, .rev, .sb, .mp, .ns").toggleClass("hide-item");

        }

  //Shows and hides the open and close menu
  $('.openclose-menu').on('click', function() {
    $(this).next(".submenu").toggleClass('hide-item');
  });

  //List all urls in the menu below
  var urltotarget = [
    '/grants/grants_process.htm',
    '/grants/grant_basics.htm',
    '/grants/get-started.htm',
    '/grants/planning_application.htm',
    '/grants/how-to-apply-application-guide/prepare-to-apply.htm',
    '/grants/how-to-apply-application-guide/write-application.htm',
    '/grants/how-to-apply-application-guide/submit.htm',
    '/grants/how-to-apply-application-guide/video/index.htm',
    '/grants/receipt-referral.htm',
    '/grants/peer-review.htm',
    '/grants/pre-award-process.htm',
    '/grants/post-award-monitoring-and-reporting.htm',
    '/grants/forms.htm',
    '/grants/information-for-research.htm',
    '/grants/information-for-research-administrators.htm',
    '/grants/policy/review.htm',
    '/grants/foreign/index.htm',
    '/grants/foreign/important-highlights.htm',
    '/grants/information-for-media-public.htm',
    '/grants/referral-and-review.htm',
    '/grants/pre-and-post-award-processes.htm',
    '/grants/information-for.htm'
  ];


  //This does the leftnav highlighting on three items that are on a sub sub level, these are coded this way because I orginally didn't plan to have the leftnav highlight two levels down
  if (window.location.href.indexOf("data-tables") > -1) {

    //$(".parentmenu-link").toggleClass("selected-parentmenu");
    //$(".submenu").toggleClass("selected-submenu");
    $(".fl").toggleClass("selected-item");
  }

  //Goes through each href in the list above and applies various items to it if it is the current url the browser is on
  //this includes highlighing background colors, hiding menu buttons, etc
  $.each(urltotarget, function(index, value) {

    if ((window.location.href.indexOf(value) > -1)) {

      $('a[href*="' + value + '"]').each(function() {


        // This sets the variable named n to be the html text in whatever is currently the URL
        var n = $(this).text().length;




        if (($(window).width() < 1920) && ($(window).width() > 1500)) {
          var m = 32;
        } else if ($(window).width() < 1500) {
          var m = 25;
        }

        if ($(window).width() < 1200) {
          var m = 20;
        }

        // If the html text of the link is below 32 characters use the smaller arrow
        if (n < m) {
          if ($(this).is(".parentmenu-link, .selected-parentmenu")) {
            $(this).append('<span class="arrow-right"></span>');
          } else {
            $(this).append('<span class="arrow-right-sub"></span>');
          }

        }


        // If the html text of the link is equal to or more than 45 characters use the smaller arrow
        if (n >= m) {

          if ($(this).is(".parentmenu-link, .selected-parentmenu")) {
            $(this).append('<span class="arrow-right-twolines"></span>');
          } else {
            $(this).append('<span class="arrow-right-sub-twolines"></span>');
          }

        }

        $(this).parent('li').toggleClass('selected-item');

        // Checks if the current url is found within one of the open and close menus and hides the open and close button while adding the background color to the parent menu
        if ($(this).parents('.parentmenu').length) {
          $(this).closest(':has(a.parentmenu-link)').find('.parentmenu-link').toggleClass('selected-parentmenu');
          $(this).closest(':has(.openclose-menu)').find('.openclose-menu').toggleClass('hide-item');
        }


        // These add the background colors and remove the open and close items when needed
        $(this).next().next('.submenu').toggleClass('selected-submenu');
        $(this).closest('.submenu').toggleClass('selected-submenu');
        $(this).next().next(".submenu").toggleClass('hide-item');
        $(this).closest(".submenu").toggleClass('hide-item');
        $(this).next('.openclose-menu').toggleClass('hide-item');
        $(this).next(".openclose-menu").toggleClass('hide-item');
        $(this).find(".openclose-menu").toggleClass('hide-item');
        $(".nav-menu").find("li.selected-item").removeClass("selected-item");

      });

    }

  });

$('.openclose-menu:last').css("margin-top", "0px");

  $(".openclose-menu").click(function() {
    $(this).text(function(i, text) {
      return text === "+" ? "-" : "+";
    })
  });
</script>

                </aside>
              </div>
            
