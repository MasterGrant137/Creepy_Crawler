                            } else {
                                logBranchMetric('BranchSDK.Error', 0);
                            }
                        });
                    }
                });
            }
        }

        function consentKnown(e) {
            return e.detail.advertising === 'approved' || e.detail.advertising === 'denied';
        }

        function consentApproved(e) {
            return e.detail.advertising === 'approved';
        }

        function disableTracking() {
            if (window && window.branch) {
                window.branch.disableTracking(true);
            }
        }

        if (false) {
            document.addEventListener('gdpr-consent', function(e) {
                if (consentKnown(e)) {
                    initializeBranch(!consentApproved(e));
                    if (!consentApproved(e)) { disableTracking(); }
                }
            });
        } else {
            initializeBranch(false);
        }
    </script>
    <script>
      if ('csm' in window) {
        csm.measure('csm_RenderBranchSDK_finished');
      }
    </script>
<script>
    if (typeof uet == 'function') {
      uet("be", "RenderBranchSDK", {wb: 1});
    }
</script>
<script>
    if (typeof uex == 'function') {
      uex("ld", "RenderBranchSDK", {wb: 1});
    }
</script>

<script>
    if (typeof uet == 'function') {
      uet("be", "LoadFooterJS", {wb: 1});
    }
</script>
<script>
    if (typeof uex == 'function') {
      uex("ld", "LoadFooterJS", {wb: 1});
    }
</script>
        
        <div id="servertime" time="20"/>



<script>
    if (typeof uet == 'function') {
      uet("be");
    }
</script>
        
    </body>
</html>
