                if (window.jQuery && typeof $.fn.watchlistRibbon !== 'undefined') {
                    jQuery(
                            function() {
        // Init the watchlist rendering
        $("a.wlb_watchlist_lite").wlb_lite();
        $(".lister").watchlistRibbon(".ribbonize");

        // Invoke the rating animation
        $(".lister-item").rating_animation();
    }

                    );
