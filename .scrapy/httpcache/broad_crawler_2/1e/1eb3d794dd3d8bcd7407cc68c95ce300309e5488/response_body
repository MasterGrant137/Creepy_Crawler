    <div class="views-element-container container-fluid" id="block-views-block-featured-scientists-block-1">
        
                    <h1 class="title white">Featured Scientists</h1>
                
                    <div class="content">
                <div block="block-views-block-featured-scientists-block-1"><div class="row">
    
    
    

          <header>
          <style>
