<script>
$(function() {
    window.prettyPrint && prettyPrint()
    $(document).on('click', '.yamm .dropdown-menu', function(e) {
        e.stopPropagation()
    })
})

$(document).ready(function() {

    /**To be DESKTOP ONLY functionality**/

    $('.level-two').hover(function() {
        $('.level-three').removeClass('open');
        $('.menu-group').removeClass('open');
        $(this).parent().addClass('open');
        $(this).parent().find('.level-three').addClass('open');
    });


    $('a.dropdown-toggle').click(function(event) {
        if ($(window).width() > 990) {
            var link_location = $(this).attr('href');
            window.location.href = link_location;
        }
    });

    $('.dropdown').hover(function(event) {
            if ($(window).width() > 990) {
                $('.dropdown').each(function() {
                    $(this).removeClass('open');
                });
                $(this).addClass('open');
            }
        },
        function() {
            $('.dropdown').each(function() {
                $(this).removeClass('open');
            });
            $('.level-three').removeClass('open');
            $('.menu-group').removeClass('open');
        });

    /**End DESKTOP ONLY functionality**/

    $('.dropdown-toggle').click(function() {
        $('.menu-group').removeClass('open');

        $('.level-three').removeClass('open');

    });

});

</script>
<script>
$(document).ready(function() {
    $('#menu').slinky();
});

</script>
<!-- tweet and share :)-->
<script>
! function(d, s, id) {
    var js, fjs = d.getElementsByTagName(s)[0];
    if (!d.getElementById(id)) {
        js = d.createElement(s);
        js.id = id;
        js.src = "https://platform.twitter.com/widgets.js";
        fjs.parentNode.insertBefore(js, fjs);
    }
}(document, "script", "twitter-wjs");

</script>
<script src="/assets/js/jquery.mobile.custom.min.js"></script>



<script>


window.addEventListener('load', function() {

    const breaker = document.querySelector(".breaker");
    const breakTag = document.createElement("br");

    const strongMargin = document.querySelector(".strong-margin");
    const btnLabel = document.querySelector(".btn-label"); 


    function resize() {
        if (window.innerWidth < 650) {
                breaker.appendChild(breakTag);
                strongMargin.style.marginLeft = "0";
                btnLabel.style.marginLeft = "35px";
        } else {
                breaker.removeChild(breakTag);
                strongMargin.style.marginLeft = "9px";
                btnLabel.style.marginLeft = "1px";
        }
    }
    window.onresize = resize;
});

</script>
