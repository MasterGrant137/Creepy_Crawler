<script>

// Replaces all links with www.era.nih.gov that were just era.nih.gov START
$(document).ready(function(){
  $('a').each(function(){
    //console.log(this.href);
    if (this.href.indexOf("era.nih.gov")) {
       this.href = this.href.replace('https://era.nih.gov', 'https://www.era.nih.gov');
    }
   });
});
// Replaces all links with www.era.nih.gov that were just era.nih.gov END 



(function ($) {
 


// Controls the open and close top banner
  $("div.expandbutton").click(function(){
    $(".expandbutton-message").toggle();
    $(".expandmessage").toggle();
    $(".collpasebutton").toggle();
    $(".expandbutton").toggle();
    
  });

  $("div.expandbutton-message").click(function(){
    $(".expandbutton-message").toggle();
    $(".expandmessage").toggle();
    $(".collpasebutton").toggle();
    $(".expandbutton").toggle();
  });

  $("div.collpasebutton").click(function(){
    $(".expandbutton-message").toggle();
    $(".expandmessage").toggle();
    $(".collpasebutton").toggle();
    $(".expandbutton").toggle();
  });

})(jQuery);
</script>

