<script type="text/javascript">
/*
 *  Returns an array of jQuery objects, grouped by specified number of elements.
 *  
 *    Say you have 17 <frameset> tags on your page...
 *  
 *    $('frameset').inGroupsOf(7); // => [ jQuery[0..6], jQuery[7..13], jQuery[14..16] ]
 *
 */
$.fn.inGroupsOf = function( countPerGroup ) {
    
    var groups = [], offset = 0, $group;
    
    while ( ($group = this.slice( offset, (countPerGroup + offset) )).length ) {
        groups.push( $group );
        offset += countPerGroup;
				
    }
	
	for( i=0; i<groups.length; i++ ){
		//   groups[i].addClass('li_col'+i);
		     //$('.li_col'+i).wrapAll('<div class="pane"><ul>');

		}
		 
    return groups;
};

$(document).ready(function() {

   

  
  
$(".cList li a").each(function () {
		
			var currentcountry = $(this);
			var countryvalue = currentcountry.text();
			var firstchar = countryvalue.charAt(0);
			var cleanfirstchar = firstchar.replace(',','').replace('(', '').replace(')','').replace('’','').replace('É','E').replace('é','E').replace('Î','I').replace('ô','O'); 
			$(this).parent().attr('class',cleanfirstchar);
  			// $(this).attr('title',firstchar);
			
	

		}); //end each
  

  $('.A, .a, .B, .b, .C, .c').wrapAll('<div class="pane"><ul>');
		$('.D, .d, .E, .e, .F, .f, .G, .g, .H, .h, .I, .i').wrapAll('<div class="pane"><ul>');
		$('.J, .j, .K, .k, .L, .l, .M, .m').wrapAll('<div class="pane"><ul>');
		$('.N, .n, .O, .o, .P, .p, .Q, .q, .R, .r').wrapAll('<div class="pane"><ul>');
		$('.S, .s, .T, .t').wrapAll('<div class="pane"><ul>');
  $('.U, .u, .V, .v, .W, .w, .X, .x, .Y, .y, .Z, .z').wrapAll('<div class="pane"><ul>');
  

  
  //$('.cList .pane li a').tsort({attr:'title', charOrder: 'a[àâ]c[ç]e[éèêë]i[ïî]o[ôœ]u[ûù]'});

  $('.cList .pane li').inGroupsOf(10);





}); //end function





 
</script>
