<script>
    async function getData(){
        const ambassadors = await fetch('/__extra/ambassadors/ambassadors.json').then(response => response.json());
        
        // This will produce a new array with 3 random elements from the original array
        const shuffled = ambassadors.sort(() => 0.5 - Math.random()).slice(0,3);

        var html = shuffled.map((ambassador) => {
            return `
                <div class="col-12 col-md-6 col-lg-4">
                    <figure class="bg-img" style="background-image: url(${ambassador.imgPath});">
                        <figcaption>
                            <div>
                                <h5>
                                    ${ambassador.name}
                                </h5>
        
                                <p>
                                    “${ambassador.quote}”
                                </p>
                            </div>
                        </figcaption>
                    </figure>
                </div>
            `;
        }).join("")
        
        document.querySelector("#ambassadorsList").innerHTML = html;
    }
    getData();
</script></div>
      
  
  </div>

