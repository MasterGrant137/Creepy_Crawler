<style>.back-to-top a {
  position: fixed;
  z-index: 999;
  bottom: 10px;
  right: 10px;
  display: inline-block;
  height: 32px;
  box-sizing: border-box;
  padding: 0 8px;
  margin-bottom: 0;
  border-radius: 0;
  font-weight: normal;
  text-align: center;
  vertical-align: middle;
  cursor: pointer;
  background-color: #0076AD;
  color: #fff;
  border: 1px solid #0076AD;
  font-size: 12px;
  font-weight: bold;
  line-height: 30px;
  padding: 7px 12px 7px 10px;
  height: auto;
  line-height: normal;
}
.back-to-top a:link,
.back-to-top a:visited,
.back-to-top a:active {
  color: #fff;
}
.back-to-top a:hover,
.back-to-top a:focus {
  background-color: #002347;
  border-color: #002347;
  color: #fff;
  text-decoration: none;
}
.back-to-top a.button-tertiary {
  color: #0076AD;
  background-color: #fff;
}
.back-to-top a.button-tertiary:link,
.back-to-top a.button-tertiary:visited,
.back-to-top a.button-tertiary:active {
  color: #0076AD;
}
.back-to-top a.button-tertiary:hover,
.back-to-top a.button-tertiary:focus {
  background-color: #fff;
  border-color: #002347;
  color: #002347;
  text-decoration: none;
}
.back-to-top a.button-gray {
  background-color: #808080;
  border-color: #808080;
}
.back-to-top a.button-gray:hover,
.back-to-top a.button-gray:focus {
  background-color: #000;
  border-color: #000;
  color: #fff;
  text-decoration: none;
}
</style>
<script>jQuery(function ($) {
    var scroll_timer,
        displayed = false,
        $message = $('.back-to-top a').hide(),
        $window = $(window),
        top = $(document.body).children(0).position().top;

    $("#back-to-top").click(function(event){
        $("#top").focus();
        $window.scrollTop(0);
        event.preventDefault();
        return false;
    });

    $window.scroll(function () {
        window.clearTimeout(scroll_timer);
        scroll_timer = window.setTimeout(function () {
            if($window.scrollTop() <= top){
                displayed = false;
                $message.fadeOut(800);
            }else if(!displayed){
                displayed = true;
                $message.stop(true, true).show().click(function(){
                    $message.fadeOut(800);
                });
            }
        }, 100);
    });

    $("#back-to-top").click(function(e){
        UX.interaction($(this), 'Footer', 'click', 'Back to top');
    });
});
</script>


