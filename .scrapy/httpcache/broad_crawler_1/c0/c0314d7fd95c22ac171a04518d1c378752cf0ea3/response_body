<script defer='defer'>
jQuery(function($) {
    $(".cite-this > h2").click(function(event){
        $('.icon', event.delegateTarget)
            .andSelf()
            .toggleClass('accordion-down accordion-up');
        $('#cite-this-content').toggle();
        if ($('.cite-this h2 a > i.icon').hasClass('accordion-down')) {
            $('.cite-this h2 a > i.icon').addClass('icon-minus-square');
        } else {
            $('.cite-this h2 a > i.icon').removeClass('icon-minus-square');
        }
        return false;
    });
});
</script>


