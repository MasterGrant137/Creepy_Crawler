    onResize = function() {
        if($(window).width() < 767) {
          $('aside.first-sidebar').appendTo('section.col-sm-6');
        } else {
            $('aside.first-sidebar').insertBefore( $( 'section.col-sm-6' ) );
        }
    }

    $(document).ready(onResize);
    $(window).resize(onResize);

    var delay = (function() {
        var timer = 0;
        return function(callback, ms){
            clearTimeout(timer);
            timer = setTimeout(callback, ms);
        }
    })();

    $(window).resize(function() {
        delay(function(){
            onResize();
        },100)
    });

});

function mobileview() {
  var x = document.getElementById("myTopnav");
  if (x.className === "topnav") {
    x.className += " responsive";
  } else {
    x.className = "topnav";
  }
}


