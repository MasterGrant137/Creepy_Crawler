<script>
  $(document).ready(function () {
    $("img").hover(function () {
      var temp = $(this).attr("src");
      $(this).attr("src", $(this).attr("data-alt-src"));
      $(this).attr("data-alt-src", temp);
    });
    $("#down-arrow").click(function () {
      $("html, body").animate(
        {
          scrollTop: $("#getstarted").offset().top,
        },
        2000
      );
    });
  });
</script>
    
                
                
