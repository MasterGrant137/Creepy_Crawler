    <script type="text/javascript">
      if(self === top) {
        var antiClickjack = document.getElementById("antiClickjack");
        antiClickjack.parentNode.removeChild(antiClickjack);
      } else {
        top.location = self.location;
      }
      var IDLE_TIMEOUT = 1800; //seconds
      var IDLE_WARNING_TIMEOUT = 1620; //seconds
      var sessionModalActive = false;
      window.setInterval(CheckIdleTime, 1000);

      var startDate = new Date();
      function CheckIdleTime() {
        var loggedIn = false;
        if(loggedIn) {
          var currentDate = new Date();
          var diffCount = currentDate - startDate;
          var minutes = Math.floor((diffCount / 60000)) % 60;
          var seconds = Math.floor((diffCount /  1000)) % 60;
          _idleSecondsCounter = (minutes * 60) + seconds;
          sessionModalActive = _idleSecondsCounter >= IDLE_WARNING_TIMEOUT;
          // session is about to timeout, modal should popup
          if(sessionModalActive) {
            remainingSessionTime = IDLE_TIMEOUT - _idleSecondsCounter;
            var minutes = Math.floor(remainingSessionTime / 60);
            var seconds = remainingSessionTime % 60;
            if(9 >= seconds) {
              seconds = '0' + seconds;
            }
            document.getElementById("remainingSessionTime").innerHTML = minutes + ':' +seconds;
            showDialog('sessionTimeoutModal');
          }
          // session timed out, user will be logged out
          if(_idleSecondsCounter >= IDLE_TIMEOUT) {
            closeDialog('sessionTimeoutModal');
            document.getElementById('signoutForm:signout').click(); 
          }
        }
      }
      function staySignedIn() {
        _idleSecondsCounter = 0;
        closeDialog('sessionTimeoutModal');
      }
    </script>
    
