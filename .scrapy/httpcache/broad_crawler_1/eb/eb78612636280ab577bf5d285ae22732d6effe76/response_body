<script>

    $(document).ready(function () {
        $("#nav-home").addClass("active");
        $('#banner').show();
        setInterval(refreshDiv, 30000);
  		refreshDiv();
  		console.log('remote: 47.158.30.50 t=0');
    });
	
	function refreshDiv()
	{
        $.ajax( {
            url: 'program.asp',
            cache: false,
            timeout: 29000,
            data: {
                t: 0,
                test: 0
            },
            dataType: 'html',
            method: 'POST'
        }).done(function (data) {
            $('#result').html(data);
        }).fail(function (jqXHR, status, summary) {
            $('#result').html("<div class=\"alert alert-danger\"><h1 class=\"alert-heading\">System is busy, one moment please...</h1></div>");
        });
	}
	
</script>

