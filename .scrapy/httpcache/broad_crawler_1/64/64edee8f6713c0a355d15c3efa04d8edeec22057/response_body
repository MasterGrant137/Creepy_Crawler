<script type='text/javascript'>  
  
  /* need to create alias because of jkmegamenu */
  var $jq = jQuery.noConflict();  
  
  $jq(function() {
	  
	    $jq("input#keywords").autocomplete({
	    	
	    	delay: 500,
	    	minLength: 1,
	    	
	    	source: function( request, response ) {
	    	
		    	var searchType = document.getElementById("jumpMenu").value;
		    	
		    	var data = new Object();
		    	data.keyword = request.term.toLowerCase();
		    	data.siteSearch = (searchType == "search-site") ? "true" : "false";
		    	
		    	var queryStr = "jp=" + JSON.stringify(data);
				
		        $jq.ajax({
		          url: "https://www.grants.gov/grantsws/Suggest",
		          dataType: "jsonp",
		          data: queryStr,
		          success: function( data ) {
		            response(data.suggestions);
		          }
		        });
	      	},
	      
	      	select: function( event, ui ) {
	      		$jq(this).val(ui.item.value);
	        	search();
	      	}
	    });
	});
	
  </script>











































