//This adds the lightbox feature to every image in the body area aka .main-container
      $('.main-container img').each(function() { // For each image


       if (!$(this).is(".nobaguettebox")) {
              var $img = $(this),
            href = $img.attr('src');
        $img.wrap('<a class="baguetteBox gallery" href="' + href + '"></a>');
    }
      });



