<script>

	function escapeDoubleQuotes(str) {
		str.replace(/['"]+/g, '');
	}
	
	function performSearch(){
		var query = '';
		
		if($("#search-keywords").val()) {
			query += $("#search-keywords").val() + "*";
		}
		
		if($("#search-filter-type").val()){
			query += ' (NIGMSRecordTypeOWSCHCS:{0}*)'.f($("#search-filter-type").val());
		}
		
		if($("#search-filter-topic").val()){
			query += ' (nigmstopicareas:{0}*)'.f($("#search-filter-topic").val());
		}
		
		if($("#search-filter-pixel").val()){
			query += ' (nigmshighresassetwidth>{0} OR nigmshighresassetheight>{0})'.f($("#search-filter-pixel").val());
		}
		location.href = '/Pages/SearchResults.aspx?' + $('#PnlSearchFilter :input').serialize() + '#Default={"k":"('  + query + '*)",}';
	}
	
	$("#search-keywords").on('keyup', function (e) {
	    if (e.keyCode == 13) {
	       performSearch();
	    }
	});


</script><html xmlns:mso="urn:schemas-microsoft-com:office:office" xmlns:msdt="uuid:C2F41010-65B3-11d1-A29F-00AA00C14882"><head>
