// These are needed because the tab through menu feature does not load the hide 
// submenu fast enough when the page loads, so the hide class is applied to all 
// the submenus and then this code removes it so it does not interfer with the
// keyboard tab feature. I know it sounds dumb but it works. ¯\_(ツ)_/¯  
 
 $(".applicants-menu").removeClass("hide");
 $(".grantees-menu").removeClass("hide");
 $(".reviewers-menu").removeClass("hide");
 $(".manage-accounts-menu").removeClass("hide");



 $('.news').hover(function(e) {
   e.preventDefault();
     $('.news-menu').stop().toggleClass('hide');
});

 $('.help-and-tutorials').hover(function(e) {
   e.preventDefault();
     $('.help-and-tutorials-menu').stop().toggleClass('hide');
});

 $('.about').hover(function(e) {
   e.preventDefault();
     $('.about-menu').stop().toggleClass('hide');
});


