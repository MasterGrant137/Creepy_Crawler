<script type="text/javascript">
  //checks whether or not if the agreement box is checked
  function isChecked() {
    let checkbox = document.getElementById("agreeToTerms"); //finds the checkbox
    let valid = false; //set validity to false by default

    if (checkbox.checked !== true) {
      document.getElementById("alertMsg").innerHTML = "You must click the checkbox to accept the rules of behavior and the subscriber agreement before continuing."; //displays error message if the agreement box isn't checked
    } else {
      valid = true; //proceeds to if it is checked
    }
    return valid;
  }
</script>

