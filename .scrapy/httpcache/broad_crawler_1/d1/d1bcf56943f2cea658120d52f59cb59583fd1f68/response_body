<script type="text/javascript">
window.addEventListener("load",function () {
	
	//Get the modal
	var modal = document.getElementById("loginGovModalDialogNative");
	
	// Get the button that opens the modal
	var btn = document.getElementById("loginGovConfirmLink");
	
	// Get the <button> element that closes the modal
	var closeBtn = document.getElementById("native-loginGovCancelButton");
	
	// When the user clicks the button, open the modal 
	btn.onclick = function() {
	  modal.style.display = "block";
	}
	
	// When the user clicks on <span> (x), close the modal
	closeBtn.onclick = function() {
	  modal.style.display = "none";
	}
	
	// When the user clicks anywhere outside of the modal, close it
	window.onclick = function(event) {
	  if (event.target == modal) {
	    modal.style.display = "none";
	  }
	}

}, false);



</script>
				
 		
 			




<style>	
	#vdsModalDialogNative{
		display:none;
	}		
</style>


<script type="text/javascript">




window.addEventListener("load",function () {
	
	document.getElementById("password-btn").onclick = function(event) {
		  var x = document.getElementById("clc_password");
		  if (x.type === "password") {
		    x.type = "text";
		  } else {
		    x.type = "password";
		  }
		  
		  event.preventDefault();
		  return false;
	};

	
	var elem = document.getElementById("password-btn").getElementsByTagName("i")[0];
	
	elem.onclick = function() {
		
		this.classList.toggle('fa-eye-slash');
		this.classList.toggle('fa-eye');
		return false;
	}
	
	
	
	//Get the modal
	var modal = document.getElementById("vdsModalDialogNative");
		
	// Get the <button> element that closes the modal
	var closeBtn = document.getElementById("native-vdsCloseButton");
	
	var btn = document.getElementById("clc_submit");

	
	// When the user clicks the button, open the modal 
	btn.onclick = function() {
	  var userName = document.getElementById("clc_user").value;
	  var password = document.getElementById("clc_password").value;
	  if(!userName || !password){
	  	modal.style.display = "block";
	  	return false;
	  }
	  
	  return true;
	}
	
	// When the user clicks on <span> (x), close the modal
	closeBtn.onclick = function() {
	  modal.style.display = "none";
	}
	
	// When the user clicks anywhere outside of the modal, close it
	window.onclick = function(event) {
	  if (event.target == modal) {
	    modal.style.display = "none";
	  }
	}
	
}, false);


</script>	

