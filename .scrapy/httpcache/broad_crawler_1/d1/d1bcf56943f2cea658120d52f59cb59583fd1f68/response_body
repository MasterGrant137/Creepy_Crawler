<script type="text/javascript">
window.addEventListener("load",function () {
	
	//Get the modal
	var mfaCredentialErrormodal = document.getElementById("mfaCredentialsErrorDialogNative");
	
	// Get the <button> element that closes the modal
	var mfaCredentialErrorOkBtn = document.getElementById("native-mfaCredentialsErrorOkButton");
	
	var mfaIncommonFederationErrormodal = document.getElementById("mfaIncommonErrorDialogNative");
	
	var mfaIncommonFederationOkBtn = document.getElementById("native-mfaIncommonErrorOkButton");
	
	//Get the modal
	var mfaCredentialInternalErrormodal = document.getElementById("mfaCredentialsInternalErrorDialogNative");
	
	// Get the <button> element that closes the modal
	var mfaCredentialInternalErrorOkBtn = document.getElementById("native-mfaCredentialsInternalErrorOkButton");
	
	var mfaCredentialsAccountLockedModal = document.getElementById("mfaCredentialsAccountLockedDialogNative");
	
	var mfaCredentialsAccountLockedOkButton =  document.getElementById("native-mfaCredentialsAccountLockedOkButton");
	
	var mfaCredentialsInvalidCredentialsModal = document.getElementById("mfaInvalidCredentialsDialogNative");
	
	var mfaCredentialsInvalidCredentialsOkButton = document.getElementById("native-mfaCredentialsInvalidCredentialsOkButton");
	
	const queryString = window.location.search;
	const urlParams = new URLSearchParams(queryString);
	
	const mfaErrorUserLoginMethod = urlParams.get('mfaErrorUserLoginMethod');
	
	//var mfaErrorUserLoginMethod = '';
	// When the user clicks the button, open the modal 
	if(mfaErrorUserLoginMethod && mfaErrorUserLoginMethod.length!=0 && mfaErrorUserLoginMethod==="credentialsExternal"){
		mfaCredentialErrormodal.style.display = "block";
	}else if(mfaErrorUserLoginMethod && mfaErrorUserLoginMethod.length!=0 && mfaErrorUserLoginMethod==="sfaInCommon"){
		mfaIncommonFederationErrormodal.style.display="block";
	}else if(mfaErrorUserLoginMethod && mfaErrorUserLoginMethod.length!=0 && mfaErrorUserLoginMethod==="credentialsInternal"){
		mfaCredentialInternalErrormodal.style.display="block";
	}else if(mfaErrorUserLoginMethod && mfaErrorUserLoginMethod.length!=0 && mfaErrorUserLoginMethod==="accountLocked"){
		mfaCredentialsAccountLockedModal.style.display="block";
	}else if(mfaErrorUserLoginMethod && mfaErrorUserLoginMethod.length!=0 && mfaErrorUserLoginMethod==="invalidCredentials"){
		mfaCredentialsInvalidCredentialsModal.style.display="block";
	}
	
	// When the user clicks on <span> (x), close the modal
	mfaCredentialErrorOkBtn.onclick = function() {
		mfaCredentialErrormodal.style.display = "none";
	}
	
	mfaIncommonFederationOkBtn.onclick = function() {
		mfaIncommonFederationErrormodal.style.display = "none";
	}
	
	mfaCredentialInternalErrorOkBtn.onclick = function() {
		mfaCredentialInternalErrormodal.style.display = "none";
	}
	
	mfaCredentialsAccountLockedOkButton.onclick = function() {
		mfaCredentialsAccountLockedModal.style.display = "none";
	}
	
	mfaCredentialsInvalidCredentialsOkButton.onclick = function() {
		mfaCredentialsInvalidCredentialsModal.style.display = "none";
	}
	
	
	// When the user clicks anywhere outside of the modal, close it
	window.onclick = function(event) {
	  if (event.target == mfaCredentialErrormodal) {
		  mfaCredentialErrormodal.style.display = "none";
	  }else if(event.target == mfaIncommonFederationErrormodal){
		  mfaIncommonFederationErrormodal.style.display = "none";
	  }else if(event.target == mfaCredentialInternalFederationErrormodal){
		  mfaCredentialInternalErrormodal.style.display = "none";
	  }else if(event.target == mfaCredentialsAccountLockedModal){
		  mfaCredentialsAccountLockedModal.style.display = "none";
	  }else if(event.target == mfaCredentialsInvalidCredentialsModal){
		  mfaCredentialsInvalidCredentialsModal.style.display = "none";
	  }
	}

}, false);



</script>
