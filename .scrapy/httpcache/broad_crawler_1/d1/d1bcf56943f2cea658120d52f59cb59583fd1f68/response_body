<script type="text/javascript">

function dropDownSubmit() {
	/* if (document.getElementById("federatedInstitutions").value != '') {
		window.location.href = document.getElementById("federatedInstitutions").value;
	} */
	if(document.getElementById("clc_organization-entityID").value!=''){
		var ssoURL = 'https://federation.nih.gov/Shibboleth.sso/Login?entityID=';
		var provURL = '&target=https%3A%2F%2Ffederation.nih.gov%2FShibAuth%2Finitiatelogin%3Ftarget%3D';
		var target = encodeURIComponent('https%3A%2F%2Fpublic.era.nih.gov%3A443%2Fassist%2F').replace(/\+/g, '%2B');
		var requestMFAUrl = 'https://refeds.org/profile/mfa';
			//var encodedTarget=getSocialtarget();
			//target=target.substring(target.indexOf("-SM-")+4);
			//var encodedTarget = encodeURIComponent(target);
		var dblEncodedTarget = encodeURIComponent(target);
		var rediurl="";
		if(requestMFAUrl && requestMFAUrl.length!=0){
			rediurl=""+ ssoURL +document.getElementById("clc_organization-entityID").value+"&SAMLDS=1&authnContextClassRef="+requestMFAUrl;
		}else{
			rediurl=""+ ssoURL +document.getElementById("clc_organization-entityID").value+"&SAMLDS=1";
		} 
		var finalurl=""+rediurl+ provURL + dblEncodedTarget
		window.location.href=""+finalurl;
	}
}

</script>	

    <!-- Participate in analytics program -->
