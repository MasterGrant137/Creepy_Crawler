<script>
const shibbolethData = new Bloodhound({
	queryTokenizer: Bloodhound.tokenizers.whitespace,
	datumTokenizer: function(datum) {
		return Bloodhound.tokenizers.whitespace(datum.name);
	},
	prefetch: {
		url: 'https://public.era.nih.gov/ams/public/discoFeedCompactJson.era',
		cache: true,
		// The data has to be transformed into a format parsable by Bloodhound
		// This functions maps the objects into smaller objects with just the name and id
		transform: function(response) {
			return $.map(response, function (item) {
				if(item) {
					return {
						name: item.name,
						id: item.entityID
					}
				}
				return null;
			});
		}
	}
});

function onShibbolethDataLoaded() {

	const typeAheadOptions = {
		minLength: 1,
		highlight: true,
	};

	const dataFeedOptions = {
		name: 'shibbolethData',
		source: shibbolethData,
		display: 'name',
		limit: 10,
		templates: {
			empty: '<p>No Results Found</p>',
			suggestion: function (data) {
				return '<p id=' + data.id + '>' + data.name + '</p>';
			}
		}
	};

	// Initalize the typeahead
	$('#clc_organization-name').typeahead(typeAheadOptions, dataFeedOptions);

	// Accessibility improvements
	// Adds some ARIA attributies to help with accessibility
	$('.tt-dataset').attr('id', 'typeAheadMenu');

	// Updating the `aria-activedescendant` on the parent input when the item changes
	// See https://github.com/twitter/typeahead.js/blob/master/doc/jquery_typeahead.md#custom-events
	function updateEvent(evt,data) {
		if(data && data.id) {
			$('#clc_organization-name').attr('aria-activedescendant', data.id)
			$('#clc_organization-entityID').val(data.id);
		}
	}
	$('#clc_organization-name').bind('typeahead:cursorchange', updateEvent);
	$('#clc_organization-name').bind('typeahead:select', updateEvent);

	//Finally, hide the loader and show the content
	$('.isLoading').hide().attr('aria-hidden', true);
	$('.hasLoaded').show().attr('aria-hidden', false);
}

// Initalize the bloodhound engine, so we can hook into the Promise lifecycle to know
// when the data is loaded
var promise = shibbolethData.initialize();

promise
.done(onShibbolethDataLoaded)
.fail(function() { console.warn('Feed failed to initalize'); })

</script>
 		
	<!--=========== CONTENT ENDS ============= -->
	</div>
	

			</div>
                    <!-- left panel ends -->
                    <div class="int_middle_panel col-sm-6 col-md-6 col-lg-6">
                        <div id="contentMgtDiv" class="fltlft leftFormMargin width95">
