	function setCssClassToNativeModalHeaderAndFooter(){
		$('div.native-modal-header > h4').addClass('dark-grey-text');
		$('ul.list-inline > li > a').addClass('dark-grey-text');
	}
	
	$(document).ready(function() {
		setCssClassToNativeModalHeaderAndFooter();
	});
