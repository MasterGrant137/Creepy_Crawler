<script type="text/javascript">



    window.onload = function () {
        document.onclick = function (e) {
            var sTops = document.getElementById("sTopMenues").value;
            if (sTops.indexOf(e.target.id))
                HideAllSubmenues()
        }
    }

    function ToggleMegamenu() {
        HideAllSubmenues();
        var x = document.getElementById("cssmenu");
        if (x.style.display==="none")
            x.style.display = "block";
        else
            x.style.display = "none";
    }

    function HideAllSubmenues() {
        var x = document.getElementsByClassName("dropdown-panel");
        for (i = 0; i <= x.length - 1; i++) {
            x[i].style.display = "none";
        }
        var m = document.getElementsByClassName("top-menu");
        for (i = 0; i <= m.length - 1; i++) {
            m[i].classList.remove("active");
        }
    }

    function OpenSubmenu(el, id) {

        HideAllSubmenues();
        el.classList.add("active");

        if (id != "") {
            document.getElementById(id).style.display = "inline-block";            
        }
    }

    function load_menu(m, ob) {

        if (ob == '')
        {
            dd = document.getElementsByClassName("dropbtn");
            for (var i = 0, il = dd.length; i < il; i++) {
                dd[i].style.display = "block";
            }
            dd = document.getElementsByClassName("sub-menu-open");
            for (var i = 0, il = dd.length; i < il; i++) {
                dd[i].style.display = "block";
            }
            dd = document.getElementsByClassName("dropbtn2");
            for (var i = 0, il = dd.length; i < il; i++) {
                dd[i].style.display = "block";
            }
            dd = document.getElementsByClassName("sub-menu-open2");
            for (var i = 0, il = dd.length; i < il; i++) {
                dd[i].style.display = "block";
            }
        } else
        {
            dd = document.getElementById('sub'+ob).getElementsByClassName("dropbtn2");
            for (var i = 0, il = dd.length; i < il; i++) {
                dd[i].style.display = "block";
            }
            dd = document.getElementById('sub'+ob).getElementsByClassName("sub-menu-open2");
            for (var i = 0, il = dd.length; i < il; i++) {
                dd[i].style.display = "block";
            }
            var b = document.getElementById("b" + ob);
            b.style.display = "block";
        }

        var d = document.getElementById("sub" + m.id.substring(1));
        d.style.display = "none";
        m.style.display = "none";
    }

    function load_submenu(m, lvl) {

        dd = document.getElementsByClassName("dropbtn" + lvl);
        for (var i = 0, il = dd.length; i < il; i++) {
            dd[i].style.display = "none";
        }
        dd = document.getElementsByClassName("sub-menu-open" + lvl);
        for (var i = 0, il = dd.length; i < il; i++) {
            dd[i].style.display = "none";
        }
        dd = document.getElementsByClassName("back-btn");
        for (var i = 0, il = dd.length; i < il; i++) {
            dd[i].style.display = "none";
        }

        var d = document.getElementById("sub" + m.id);
        d.style.display = "block";
        d = document.getElementById("b" + m.id);
        d.style.display = "block";

        dd = d.getElementsByClassName("dropbtn2");
        for (var i = 0, il = dd.length; i < il; i++) {
            dd[i].style.display = "block";
        }

    }


    var inp = document.getElementById('srch_box');
    var inp2 = document.getElementById('srch_box2');
    var icon = document.getElementById('SearchIcon');
    var box = document.getElementById('SearchBox');

    function execute_search() {
        var s = 'https://www.nigms.nih.gov/';
        if (inp.value.trim() != "")
            document.location = s + '/Search/Pages/results.aspx?k=' + inp.value;
        else if (inp2.value.trim() != "")
            document.location = s + '/Search/Pages/results.aspx?k=' + inp2.value;

    }

    if (inp.parentNode.tagName.toString().toLowerCase() == 'div') {
        inp.onfocus = function () {
            this.parentNode.style.backgroundColor = '#fff';
            this.parentNode.style.height = '44px';
            this.parentNode.style.marginTop = '0px';
            this.parentNode.style.marginLeft = '0px';
            icon.style.color = '#0b3268';
            inp.placeholder = "";
            this.parentNode.style.backgroundImage = "url('../../Content/images/nigms/zoom-dark.png')";
        }
        inp.onblur = function () {
            this.parentNode.style.backgroundColor = 'transparent';
            this.parentNode.style.height = '36px';
            this.parentNode.style.marginTop = '4px';
            this.parentNode.style.marginLeft = '60px';
            icon.style.color = '#fff';
            inp.placeholder = "Search ...";
            this.parentNode.style.backgroundImage = "url('../../Content/images/nigms/zoom-light.png')";
        }
    }
</script>




    
