302,&quot;meganav_style&quot;:&quot;&quot;,&quot;features&quot;:[],&quot;short_description&quot;:null},{&quot;name&quot;:&quot;NASA Science&quot;,&quot;link&quot;:&quot;/nasa_science/science/&quot;,&quot;target&quot;:&quot;_self&quot;,&quot;sections&quot;:[{&quot;items&quot;:[{&quot;name&quot;:&quot;Science Mission Directorate&quot;,&quot;link&quot;:&quot;https://science.nasa.gov/&quot;,&quot;target&quot;:&quot;_self&quot;,&quot;id&quot;:1339},{&quot;name&quot;:&quot;NASA Data Resources&quot;,&quot;link&quot;:&quot;/nasa_science/data&quot;,&quot;target&quot;:&quot;_self&quot;,&quot;id&quot;:1403},{&quot;name&quot;:&quot;Earth System Science&quot;,&quot;link&quot;:&quot;/nasa_science/science&quot;,&quot;target&quot;:&quot;_self&quot;,&quot;id&quot;:257},{&quot;name&quot;:&quot;Earth Science Missions&quot;,&quot;link&quot;:&quot;/nasa_science/missions&quot;,&quot;target&quot;:&quot;_self&quot;,&quot;id&quot;:2},{&quot;name&quot;:&quot;History&quot;,&quot;link&quot;:&quot;/nasa_science/history&quot;,&quot;target&quot;:&quot;_self&quot;,&quot;id&quot;:11},{&quot;name&quot;:&quot;People&quot;,&quot;link&quot;:&quot;/nasa_science/people&quot;,&quot;target&quot;:&quot;_self&quot;,&quot;id&quot;:1357}]}],&quot;id&quot;:1338,&quot;meganav_style&quot;:&quot;&quot;,&quot;features&quot;:[],&quot;short_description&quot;:&quot;&quot;}],&quot;search&quot;:true}" data-react-cache-id="Meganav-0" class="site_nav"></nav>
